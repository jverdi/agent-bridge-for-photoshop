{
  "id": "fcf89019",
  "name": "Agent Bridge for Photoshop",
  "version": "0.1.3",
  "main": "index.html",
  "manifestVersion": 5,
  "host": {
    "app": "PS",
    "minVersion": "24.0.0"
  },
  "requiredPermissions": {
    "network": {
      "domains": "all"
    },
    "localFileSystem": "fullAccess"
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "agent-bridge-for-photoshop-panel",
      "label": "Agent Bridge",
      "minimumSize": {
        "width": 220,
        "height": 180
      },
      "maximumSize": {
        "width": 1200,
        "height": 1400
      },
      "preferredDockedSize": {
        "width": 300,
        "height": 420
      },
      "preferredFloatingSize": {
        "width": 360,
        "height": 560
      },
      "icons": [
        {
          "width": 23, 
          "height": 23, 
          "path": "icons/panel-icon.png", 
          "scale": [ 1, 1.25, 1.5, 2 ],
          "theme": [ "all" ]
        }
      ]
    }
  ],
  "icons": [
    {
      "width": 23, 
      "height": 23, 
      "path": "icons/icon.png", 
      "scale": [ 1, 2 ],
      "theme": [ "all" ]
    }
  ]
}