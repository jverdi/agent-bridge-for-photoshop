{
  "manifestVersion": 5,
  "id": "fcf89019",
  "name": "PS Agent Bridge",
  "version": "0.1.3",
  "host": [
    {
      "app": "PS",
      "minVersion": "24.0.0"
    }
  ],
  "entrypoints": [
    {
      "type": "panel",
      "id": "psagent-panel",
      "label": {
        "default": "PS Agent Bridge"
      },
      "minimumSize": {
        "width": 220,
        "height": 180
      },
      "maximumSize": {
        "width": 1200,
        "height": 1400
      },
      "preferredDockedSize": {
        "width": 300,
        "height": 420
      },
      "preferredFloatingSize": {
        "width": 360,
        "height": 560
      },
      "icons": [
          {
              "width": 23, "height": 23, "path": "icons/icon.png", "scale": [ 1, 2 ],
              "theme": [ "darkest", "dark" ]
          }, {
              "width": 23, "height": 23, "path": "icons/icon.png", "scale": [ 1, 2 ],
              "theme": [ "lightest", "light" ]
          }
      ]

    }
  ],
  "icons": [
    {
        "width": 23,
        "height": 23,
        "path": "icons/icon.png",
        "scale": [
            1,
            2
        ]
    }
  ],
  "main": "index.html",
  "requiredPermissions": {
    "network": {
      "domains": "all"
    },
    "localFileSystem": "fullAccess"
  }
}
